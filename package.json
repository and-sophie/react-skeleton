{
  "name": "andigital.catalogue.web",
  "version": "1.0.0",
  "description": "This is the repository for the ANDcatalogue web application built in React.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "feature-test": "./node_modules/.bin/nightwatch test/feature",
    "unit-test": "mocha --compilers js:babel-register --require ./test/test_helper.js --recursive",
    "clean": "rm -rf ./build/",
    "start": "node server.js",
    "build": "ENVIRONMENT=production node node_modules/webpack/bin/webpack.js -p && cp src/index.html build/",
    "dev": "ENVIRONMENT=development node node_modules/webpack/bin/webpack.js -d --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANDigital/andigital.catalogue.web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ANDigital/andigital.catalogue.web/issues"
  },
  "homepage": "https://github.com/ANDigital/andigital.catalogue.web#readme",
  "dependencies": {
    "express": "^4.14.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sinon": "^2.0.0-pre.6",
    "superagent": "^3.5.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
